package Wall
import SetupObject
import initlater InstantBuild

public class Wall extends StaticSetupObject
	int tId = 0
	construct(vec2 pos, player owner, real angle, int tId)
		super(getUnit(owner, buildorders[tId], pos, (angle).fromDeg(), this), getUnit(owner, buildorders[tId], pos, (angle).fromDeg(), this), 0., null)
		this.tId = tId

	override function serialize() returns Json
		let json = super.serialize()
		json..addProperty(new Property(KEY_TYPE, tId.toString()))
		return json