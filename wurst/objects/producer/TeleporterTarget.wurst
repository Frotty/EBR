package TeleporterTarget
import SetupObject
constant int ID = 'u00B'


public class TeleporterTarget extends SetupObject
	
	construct(vec2 pos, player owner)
		super(getUnit(owner, ID, pos, angle(0), this), getUnit(owner, ID, pos, angle(0), this), 0.)

	override function serialize() returns Json
		let json = super.serialize()
		json
		..addProperty(new Property(KEY_TYPE, TP_TARGET_INDEX.toString()))
		return json

	override function setEnabled(boolean b)