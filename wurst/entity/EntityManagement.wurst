package EntityManagement
	import Entity
	

	function updateEntities()
		Entity e = Entity.first
		while e != null
			if e.done
				destroy e
			else if e.active
				e.update()
			e = e.next

			
		
	public function init_EntityManagement() returns boolean
		TimerStart(CreateTimer(), ANIMATION_PERIOD, true, function updateEntities)
		return true

		
endpackage
